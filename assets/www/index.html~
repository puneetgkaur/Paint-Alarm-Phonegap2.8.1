<script>
var stroke_size_onfocus=0;
function set_started_stroke_size()
{
		stroke_size_onfocus=1;
}

function set_canvas_stroke_size()
{
	if(stroke_size_onfocus==1)
	{
                var ctx=(document).getElementById("canvas_paint1 ").getContext("2d");
		ctx.lineWidth=(document).getElementById('enter_stroke_size').value;
		stroke_size_onfocus=0;
	}
}

function set_stroke_size()
{
	var e = document.getElementById("select_stroke_size");
	var str_selected = e.options[e.selectedIndex].value;
	if (str_selected=="enter a value")
        {/*
                var ctx=(document).getElementById("canvas_paint").getContext("2d");
		ctx.lineWidth=(document).getElementById('enter_stroke_size').value;	*/
        }
        else
        {
                var ctx=(document).getElementById("canvas_paint1").getContext("2d");
		var e = (document).getElementById("select_stroke_size");
		var str_selected = e.options[e.selectedIndex].value;
		ctx.lineWidth=str_selected;
	}
}

function set_enter_own_stroke_size()
{
	var e = document.getElementById("select_stroke_size");
	var str_selected = e.options[e.selectedIndex].value;
	//var textbox = document.getElementById("select_size_text");
        //textbox.toggle();
	if (str_selected=="enter a value")
        {
		unhider();
        }
        else
        {
		hider();
	}
	    function hider(){
		(document).getElementById('enter_stroke_size').style.display="none";
		//(document).getElementById('showDiv').style.display="inline-block";
	    }
	    function unhider(){
		//(document).getElementById('showDiv').style.display="none";
		(document).getElementById('enter_stroke_size').style.display="inline-block";                
	    }

