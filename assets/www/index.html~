        var canvas_image_path;
	function saveImage()
	{
		var canvas = document.getElementById('canvas_paint');
/*		window.savephotoplugin(canvas,"image/png",device.version,function(val){ 
		  //returns you the saved path in val	
				alert("Photo Saved: " + val);	
		});
*/
		var imageData=canvas.toDataURL();
		alert(imageData);
/*
    window.plugins.base64ToPNG.saveImage(myBase64, {filename:"dot.png", overwrite: true}, 
       function(result) {
          alert(result);
       }, function(error) {
          alert(error);
    });
    function saveImage(b64String, params, win, fail) {
        cordovaRef.exec(win, fail, "Base64ToPNG", "saveImage", [b64String, params]);
    };
*/

		function success_saveImage(result){
                        canvas_image_path=result;
			alert("result of save image : " + result);
		}
		function error_saveImage(error){
			alert("save image - error : " + error);
		}
                cordova.exec(success_saveImage,error_saveImage,"Base64ToPNG","saveImage",[imageData,{filename:"abc1.png",overwrite:'true'}]);

	}

</script>        

